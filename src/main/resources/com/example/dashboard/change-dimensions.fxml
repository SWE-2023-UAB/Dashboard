<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<DialogPane prefHeight="380.0" prefWidth="360.0" stylesheets="@Styling.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dashboard.ChangeDimensionsController">
   <content>
      <AnchorPane prefHeight="386.0" prefWidth="372.0">
         <children>
            <Label alignment="CENTER" layoutX="35.0" layoutY="31.0" prefHeight="17.0" prefWidth="302.0" text="Change Dimensions" textAlignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label layoutX="54.0" layoutY="124.0" prefHeight="17.0" prefWidth="72.0" text="Length:" />
            <Label layoutX="54.0" layoutY="160.0" prefHeight="17.0" prefWidth="72.0" text="Width:" />
            <Label layoutX="54.0" layoutY="196.0" prefHeight="17.0" prefWidth="72.0" text="Height:" />
            <TextField fx:id="lengthField" layoutX="111.0" layoutY="120.0" onKeyTyped="#changeLength" />
            <TextField fx:id="widthField" layoutX="111.0" layoutY="155.0" onKeyTyped="#changeWidth" />
            <TextField fx:id="heightField" layoutX="111.0" layoutY="191.0" onKeyTyped="#changeHeight" />
         </children>
      </AnchorPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="FINISH" />
   </buttonTypes>
</DialogPane>
